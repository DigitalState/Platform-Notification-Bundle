{
    "name": "digitalstate/platform-notification-bundle",
    "type": "symfony-bundle",
    "description": "DigitalState Notifications Bundle",
    "homepage": "https://github.com/DigitalState/Platform-Notification-Bundle.git",
    "keywords": ["Oro", "Notifications"],
    "license": "MIT",
    "authors": [
        {
            "name": "DigitalState, Inc",
            "homepage": "http://www.digitalstate.ca"
        }
    ],
    "require": {
        "php": ">=5.6",
        "digitalstate/platform-admin-bundle" : "dev-master",
        "digitalstate/platform-entity-bundle" : "dev-master",
        "digitalstate/platform-user-bundle" : "dev-master",
        "digitalstate/platform-security-bundle" : "dev-master",
        "digitalstate/platform-widget-bundle" : "dev-master"
    },
    "autoload": {
        "psr-4": { "Ds\\Bundle\\NotificationBundle": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}